tbody(id="result-container") 
        mixin tableRow(col1_value,col2_value,col3_value,col4_value,col5_value,col6_name,col6_date,col7_value,col8_value)
         -var is_checked= (col1_value=="false" ? 'checked': '')
         tr(class="cursor-pointer hover:bg-custom-grey") 
          td(class=".col1-div  items-center justify-center border-[0.9px] border-solid border-[#e0e3eb] h-13.2 w-[53px]  z-10")
              div(class="w-[53px]  flex items-center justify-center min-h-13.2")
                label(class="relative inline-block w-[26px] h-5 border-none")
                  input(class="sr-only peer")(type="checkbox") 
                  div(class="w-full h-full bg-[#F3EFEF] rounded-full peer-checked:bg-purple-600 transition duration-300")
                  div(class="absolute left-1 top-1 bottom-3 right-1 w-3 h-3 bg-white rounded-full transition-transform duration-300 transform peer-checked:translate-x-4")
          td(class=".col2-div border-[0.9px] border-solid border-[#e0e3eb] pl-2.5  min-h-13.2 text-[14px] w-[283px] ")
              span= col2_value  
          td(class=".col3-div border-[0.9px] border-solid border-[#e0e3eb]  pl-2 min-h-13.2 text-[12px] padding w-[133px]")
              span(class="col3-div-span  rounded-[10px] px-[5px] py-[3px]  border-[0.5px] border-solid border-[#E0E3EB] text-[#344054] bg-[#F5F6FA]")= col3_value
          td(class="pl-2.5  border-[0.9px] border-solid border-[#e0e3eb] w-[223px]")
            div(class="flex gap-[3px]")
              for imageName of col4_value 
                if imageName=="Telegram"
                  img(class="border border-solid  border-custom-img-border rounded-[3px] p-[4px]")(src='./icons/telegram.webp' width='22' height='22' alt='gmail icon')
                else if imageName=="fb" 
                  img(class="border border-solid  border-custom-img-border rounded-[3px] p-[4px]")(src='./icons/fb.png' width='22' height='22' alt='gmail icon')
                else if imageName=="slack"
                  img(class="border border-solid  border-custom-img-border rounded-[3px] p-[4px]")(src='./icons/slack.png' width='22' height='22' alt='gmail icon')
                else if imageName=="whats"  
                  img(class="border border-solid  border-custom-img-border rounded-[3px] p-[4px]")(src='./icons/whats.png' width='22' height='22' alt='gmail icon')
                else 
                  img(class="border border-solid  border-custom-img-border rounded-[3px] p-[4px]")(src='./icons/gmail.png' width='22' height='22' alt='gmail icon')

          td(class=" min-h-13.2 pl-2.5 border-[0.9px] border-solid border-[#e0e3eb] w-[163px] ")
            div
          td(class=" name-time-container items-center min-h-13.2 border-[0.9px] w-[205px] border-solid border-[#e0e3eb] pl-2.5 text-sm p-2 h-4.5 text-[#475467] ")
              div(class="flex h-full w-full items-center gap-[6px] justify-left")
                div(class="name-time-container-left")
                  span(class="name-initials p-[4px] px-[6px]  rounded-full text-[14px] text-[##667085] bg-[#EDEFF4] text-[#667085] font-semibold border-[0.8px] border-solid border-gray-200")=col6_name[0].toUpperCase()
                div(class="name-time-container-right flex flex-col tracking-[0.1px]")
                  div(class="text-[#344054] font-light")=col6_name
                  div(class=" text-[12px] font-light date2 text-[#667085] tracking-[0.3px]")= col6_date 
                  
          td(class="name-time-container.flex.row-align-only min-h-13.2 border-[0.9px] w-[205px] border-solid border-[#e0e3eb]  pl-2.5 text-sm text-[#475467]")
              div(class="flex h-full w-full items-center gap-[6px] justify-left")
                div(class="name-time-container-left")
                  span(class="name-initials p-[5px] px-[7px]  rounded-full text-[14px] text-[#667085] bg-[#EDEFF4] font-semibold")=col7_value[0].toUpperCase()
                div(class="name-time-container-right flex flex-col tracking-[0.1px]")
                  div(class="text-[#344054] font-light")=col7_value
                  div(class="text-[12px] font-light date2 text-[#667085] tracking-[0.3px]")=col8_value
          td(class="border-[0.9px] border-solid border-[#e0e3eb]  min-h-13.2 w-[61px]")
          td(class="border-[0.9px] border-solid border-[#e0e3eb]  min-h-13.2 w-[42px]")
        for dataRow of data 
          +tableRow(dataRow.Running, dataRow.Automation_name,dataRow.Trigger,dataRow.Apps_used,'',dataRow.Created_by,'26/12/24 13:45',dataRow.Last_modified_by,dataRow.Modified_at)
        
        

        